<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" 
	controllerName="com.sap.build.standard.untitledProject.controller.View2" xmlns:html="http://www.w3.org/1999/xhtml">
	
		
			<Page title="Add New XREF" showNavButton="true" navButtonPress="onNavBack">
				<content>
					<Table items="{/Products}" id="Tab1" mode="MultiSelect">
						<infoToolbar>
							<Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
								<content>
									<Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</content>
							</Toolbar>
						</infoToolbar>
						<headerToolbar>
							<Toolbar width="auto" height="46px" design="Transparent" visible="true" enabled="true">
								<content>
									<ToolbarSpacer width=""/>
									<Button text="Delete" id="DeleteButtonview2" icon="sap-icon://delete" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="DeleteEntry"/>
									<Button text="Add" id="AddButtonView2" icon="sap-icon://add" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="addEntry"/>
									<Button text="Save" id="SaveButtonView2" icon="sap-icon://save" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
										press="_onButtonPress1"/>
									<Button  text="Cancel" id="CancelButtonView2" icon="sap-icon://sys-cancel" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
										press="_onButtonPress2"/>
								</content>
								<tooltip/>
							</Toolbar>
						</headerToolbar>
						<columns>

							<Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="ID" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="SOURCE" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="X-REF NAME" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="INPUT" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="OUTPUT" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Input value="{ID}" width="auto"/>
									<Input value="{SOURCE}" width="auto"/>
									<Input value="{XREF}" width="auto"/>
									<Input value="{INPUT}" width="auto"/>
									<Input value="{OUTPUT}" width="auto"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
				
				<footer>
            <Bar design="Auto">
                <contentLeft/>
                <contentMiddle/>
                <contentRight>
                    <Button  text="Add" id="AddButtonFooterView2"  icon="sap-icon://add" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="addEntry"/>
                    <Button   text="Save"  id="SaveButtonFooterView2" icon="sap-icon://save" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="SaveButton"/>
                    <Button   text="Cancel" id="CancelButtonFooterView2" icon="sap-icon://sys-cancel" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress7"/>
                </contentRight>
            </Bar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    
			</Page>
		
	
</mvc:View>