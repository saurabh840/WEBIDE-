<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:t="sap.ui.table" xmlns:core="sap.ui.core"
	xmlns="sap.m" controllerName="TysonCheck.controller.View1" displayBlock="true">
	<App>
		<pages>
			<Page title="Add New XREF">
				<content>
					<Table items="{/Products}" id="Tab1" width="100%" noDataText="No data" mode="None" showSeparators="All" growing="true" growingThreshold="20"
						growingScrollToLoad="true">
						<infoToolbar>
							<Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
								<content>
									<Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</content>
							</Toolbar>
						</infoToolbar>
						<headerToolbar>
							<Toolbar width="auto" height="46px" design="Transparent" visible="true" enabled="true">
								<content>
									<ToolbarSpacer width=""/>
									<Button id="AddButton" icon="sap-icon://add" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="addEntry"/>
									<Button id="SaveButton" icon="sap-icon://save" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
										press="_onButtonPress1"/>
									<Button id="CancelButton" icon="sap-icon://sys-cancel" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
										press="_onButtonPress2"/>
								</content>
								<tooltip/>
							</Toolbar>
						</headerToolbar>
						<columns>
							<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="ID" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="SOURCE" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="X-REF NAME" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="INPUT" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
							<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
								<header>
									<Text text="OUTPUT" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
								</header>
								<footer/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Input value="{ID}" width="auto"/>
									<Input value="{SOURCE}" width="auto"/>
									<Input value="{XREF}" width="auto"/>
									<Input value="{INPUT}" width="auto"/>
									<Input value="{OUTPUT}" width="auto"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>